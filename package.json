{
  "name": "pixelcity",
  "version": "1.0.0",
  "packageManager": "yarn@3.1.1",
  "main": "index.js",
  "license": "MIT",
  "author": "Omar <https://twitter.com/Naomsa666>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev:ui": "yarn workspace @pixelcity/ui run dev",
    "build:ui": "yarn workspace @pixelcity/ui run build",
    "build:core": "yarn workspace @pixelcity/core run build",
    "deploy:core": "yarn workspace @pixelcity/core run deploy",
    "test:core": "yarn workspace @pixelcity/core run test",
    "chain:core": "yarn workspace @pixelcity/core run chain"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "husky": "^7.0.4"
  }
}
